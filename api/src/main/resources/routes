PUT /jobs/*id dcos.metronome.api.v1.controllers.JobSpecController.updateJob(id)
DELETE /jobs/*id dcos.metronome.api.v1.controllers.JobSpecController.deleteJob(id)
POST /jobs/*id dcos.metronome.api.v1.controllers.JobSpecController.triggerJob(id)


